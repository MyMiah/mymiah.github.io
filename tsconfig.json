{
  "extends": "astro/tsconfigs/strict",
  "include": [".astro/types.d.ts", "**/*"],
  "exclude": ["dist"],
  "compilerOptions": {
    "paths": {
        "@/*": ["./src/*"],
        "@public/*": ["./public/*"],
        "@assets/*": ["./public/assets/*"],
      },
      "baseUrl": ".",
      "strictNullChecks": true,
      "allowJs": true
  }
}